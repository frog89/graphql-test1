type Employee {
  id: ID!
  firstName: String
  lastName: String
  salary: Float
  job: Job
}

type Job {
  id: ID!
  jobTitle: String!
  minSalary: Int
  maxSalary: Int
  employees: [Employee]
}

union All = Employee | Job

type Query {
  allEmps(testIdCount: Int, salaryBetweenLower: Float, salaryBetweenHigher: Float): [Employee]
  allJobs: [Job]
  allAll: [All]
}

schema {
  query: Query
}