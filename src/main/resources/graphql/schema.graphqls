type Job {
  id: ID!
  jobTitle: String!
  minSalary: Int
  maxSalary: Int
  employees: [Employee]
}

union All = Employee | Job

type Query {
  allEmps: [Employee]
  allJobs: [Job]
  allAll: [All]
}

schema {
  query: Query
}